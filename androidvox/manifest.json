{
  "name": "ChromeVox",
  "version": "1.0",
  "description": "ChromeVox - Giving Voice to Android WebViews (DEVELOPMENT ONLY).",
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "all_frames": true,
      "js": [
        "build/build_defs.js",
        "build/build_config_android_dev.js",
        "closure/closure_preinit.js",
        "closure/base.js",
        "chromevox/injected/deps.js",
        "chromevox/injected/main.js"
      ]
    }
  ]
}
